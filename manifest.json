{
  "manifest_version": 3,
  "name": "Google Form Autofiller",
  "version": "1.0",
  "description": "Automatically fill Google Forms with saved data",
  "permissions": ["storage", "scripting", "activeTab"],
  "host_permissions": ["https://docs.google.com/forms/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/forms/*"],
      "js": ["content.js"]
    }
  ]
}
